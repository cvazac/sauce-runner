{
  "name": "sauce-runner",
  "version": "0.0.1",
  "description": "run arbitrary code across a swath of browsers using SauceLabs",
  "main": "index.js",
  "dependencies": {
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "karma": "^1.3.0",
    "karma-mocha": "^1.3.0",
